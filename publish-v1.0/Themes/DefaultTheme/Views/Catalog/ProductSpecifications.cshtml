@model IList<ProductSpecificationModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="box-bd">
        <table>
            <thead>

            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    var item = Model[i];
                    <tr>
                        <th>@item.SpecificationAttributeName</th>
                        <td>@item.SpecificationAttributeOption</td>
                        <th class="th1"></th>
                        <td class="td1"></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}